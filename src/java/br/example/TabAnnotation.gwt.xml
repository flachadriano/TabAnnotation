<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">

<module>
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name='com.gwtplatform.mvp.Mvp' />

    <entry-point class="br.example.client.TabAnnotationEntryPoint"/>

    <set-configuration-property name="gin.ginjector" value="br.example.client.injector.TabGinjector"/> 
    <set-configuration-property name="verifyNewerVersion" value="false"/> 
</module>
